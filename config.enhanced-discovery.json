{
  "service": {
    "name": "Asset Management Service - Enhanced Discovery",
    "run_as_daemon": true,
    "scan_interval": "10m",
    "auto_start": true,
    "enable_web_ui": false,
    "web_ui_port": 8080
  },
  "network": {
    "interface": "auto",
    "auto_detect_local": true,
    "default_cidr": "192.168.123.0/24",
    "scan_local_network": true,
    "scan_file_list": true,
    "enable_ipv6": false,
    "custom_dns_servers": ["8.8.8.8", "8.8.4.4"]
  },
  "arp": {
    "enabled": true,
    "timeout": "5s",
    "workers": 100,
    "rate_limit": "50ms",
    "retry_count": 3,
    "enable_vendor": true
  },
  "port_scan": {
    "enabled": true,
    "timeout": "3s",
    "workers": 100,
    "scan_tcp": true,
    "scan_udp": true,
    "common_ports": [21, 22, 23, 25, 53, 80, 110, 135, 139, 143, 443, 445, 993, 995, 1433, 3306, 3389, 5432, 5900, 8080, 8443],
    "custom_ports": [1024, 1025, 1433, 1521, 2049, 3306, 5432, 5900, 6379, 8080, 8443, 9000],
    "enable_banner": true,
    "service_detection": true,
    "scan_all_ports": false,
    "port_range_start": 1,
    "port_range_end": 1024
  },
  "discovery": {
    "enable_icmp_ping": true,
    "enable_tcp_syn_discovery": true,
    "icmp_timeout": "3s",
    "icmp_workers": 200,
    "tcp_discovery_ports": [22, 80, 443, 135, 139, 445],
    "combine_methods": true
  },
  "files": {
    "ip_list_file": "list.txt",
    "output_file": "assets.json",
    "log_file": "asset_manager.log",
    "database_file": "assets.db",
    "backup_enabled": true,
    "backup_interval": "24h"
  },
  "logging": {
    "level": "debug",
    "enable_console": true,
    "enable_file": true,
    "max_file_size": "100MB",
    "max_backups": 5,
    "enable_syslog": false
  }
} 