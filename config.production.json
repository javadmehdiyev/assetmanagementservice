{
  "service": {
    "name": "Asset Management Service - Production",
    "run_as_daemon": true,
    "scan_interval": "15m",
    "auto_start": true,
    "enable_web_ui": true,
    "web_ui_port": 8443
  },
  "network": {
    "interface": "auto",
    "auto_detect_local": false,
    "default_cidr": "10.0.0.0/8",
    "scan_local_network": true,
    "scan_file_list": true,
    "enable_ipv6": false,
    "custom_dns_servers": ["8.8.8.8", "1.1.1.1"]
  },
  "arp": {
    "enabled": true,
    "timeout": "3s",
    "workers": 10,
    "rate_limit": "50ms",
    "retry_count": 3,
    "enable_vendor": true
  },
  "port_scan": {
    "enabled": true,
    "timeout": "5s",
    "workers": 100,
    "scan_tcp": true,
    "scan_udp": true,
    "common_ports": [21, 22, 23, 25, 53, 80, 110, 135, 139, 143, 443, 445, 993, 995, 1433, 3306, 3389, 5432, 5900, 8080, 8443],
    "custom_ports": [8000, 8001, 8002, 9000, 9001],
    "enable_banner": true,
    "service_detection": true,
    "scan_all_ports": false,
    "port_range_start": 1,
    "port_range_end": 65535
  },
  "files": {
    "ip_list_file": "/etc/assetmanager/targets.txt",
    "output_file": "/var/log/assetmanager/assets.json",
    "log_file": "/var/log/assetmanager/service.log",
    "database_file": "/var/lib/assetmanager/assets.db",
    "backup_enabled": true,
    "backup_interval": "12h"
  },
  "logging": {
    "level": "warn",
    "enable_console": false,
    "enable_file": true,
    "max_file_size": "500MB",
    "max_backups": 10,
    "enable_syslog": true
  }
} 