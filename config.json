{
  "service": {
    "name": "Asset Management Service",
    "run_as_daemon": true,
    "scan_interval": "5m",
    "auto_start": true
  },
  "network": {
    "interface": "auto",
    "auto_detect_local": true,
    "default_cidr": "192.168.123.0/24",
    "scan_local_network": true,
    "scan_file_list": true
  },
  "arp": {
    "enabled": true,
    "timeout": "1s",
    "workers": 10,
    "rate_limit": "50ms"
  },
  "port_scan": {
    "enabled": true,
    "timeout": "2s",
    "workers": 20
  },
  "public_scan": {
    "enabled": true,
    "timeout": "5s",
    "workers": 10,
    "tcp_ports": [22, 23, 53, 80, 443, 993, 995, 3389, 5432, 3306],
    "udp_ports": [53, 123, 161, 514],
    "ping_enabled": true
  },
  "files": {
    "ip_list_file": "list.txt",
    "output_file": "assets.json"
  }
} 
